{
  "name": "esbpack",
  "version": "0.0.0",
  "description": "Yet another bundler for websites with esbuild",
  "author": "Kanon Kakuno <yadex205@outlook.jp>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yadex205/esbpack.git"
  },
  "bugs": {
    "url": "https://github.com/yadex205/esbpack/issues"
  },
  "homepage": "https://github.com/yadex205/esbpack#readme",
  "engines": {
    "node": ">=12.4.0"
  },
  "scripts": {
    "build": "concurrently -n lib,dts 'npm:build-lib' 'npm:build-dts'",
    "build-lib": "ts-node -T ./scripts/build.ts",
    "build-dts": "dts-bundle-generator --project ./tsconfig.dts.json --out-file dist/index.d.ts src/index.ts"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.7.13",
    "@types/pino": "^6.3.11",
    "concurrently": "^6.2.1",
    "dts-bundle-generator": "^5.9.0",
    "esbuild": "^0.12.25",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "esbuild": "^0.12.25"
  },
  "dependencies": {
    "fastify": "^3.21.0",
    "fastify-websocket": "^4.0.0",
    "mime-types": "^2.1.32",
    "pino": "^6.13.2",
    "pino-pretty": "^6.0.0"
  }
}
